package com.exceptions;

public class PxUniquenessException extends Exception {
	private static final long serialVersionUID = 3405841212935112525L;

	private String field;
	
	public PxUniquenessException(String field) {
        super();
        this.field = field;
    }
	
	public String getField() {
		return field;
	}
}
