package com.paytronix.client.android.api.exception;

public class PxException extends Exception {
    private static final long serialVersionUID = 6459560879064687977L;
    
    private String response;
    
    public PxException(String message, String response) {
        super(message);
        this.response = response;
    }
    
    public String getResponse() {
        return response;
    }
}
