package com.paytronix.client.android.api.exception;

public class PxInsufficientTokenScopeException extends Exception {
    private static final long serialVersionUID = -3858933897016432281L;
    
    private String response;
    
    public PxInsufficientTokenScopeException(String message, String response) {
        super(message);
        this.response = response;
    }
    
    public String getResponse() {
        return response;
    }
}
